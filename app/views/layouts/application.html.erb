<!DOCTYPE html>
<html>
<head>
    <title><%= AGEX_APP_NAME %></title>
  	<%= netzke_init %>

	<%= stylesheet_link_tag    "application" %>
	<%= javascript_include_tag "application" %>
	<%= csrf_meta_tags %>
</head>

<body id="container">
	<!-- Main menu section: -->
	<%= render( :partial => AGEX_APP + '/main_menu' ) %>

    <!-- Central section: -->
    <div id="container_main" class="container-fluid">
	    <div class="row-fluid">
		    <div class="span12">
			    <!--Body content-->
				<%= yield %>
		    </div>
	    </div>
    </div>

    <!-- Footer section: -->
    <%= render( :partial => AGEX_APP + '/footer_section') %>
</body>
</html>
